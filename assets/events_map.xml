<?xml version="1.0"?>
<eventsMap>
	<initialization>
		<dataSourceID></dataSourceID>
		<trackingModes>
			<mode name="beacon" value="false" time="5" /><!-- time in seconds -->
			<mode name="quartile" value="false" />
		</trackingModes>
	</initialization>
	
	
	<events>
		<!-- VIDEO SPECIFIC EVENTS -->
		<event name="mediaBegin">
			<tag name="eventName" value="Media Begin" />
			<tag name="clipName" value="{video.displayName}" />
			<tag name="clipType" value="Brightcove" />
			<tag name="currentPhase" value="Content" />
			<tag name="clip_v" value="{percent complete}" />
		</event>
	
		<event name="mediaComplete">
			<prop number="1" value="{video.customFields['my-custom-field']}" />
			<prop number="2" value="{video.customFields['showname']}" />
			<prop number="3" value="{video.displayName}" />
			<prop number="4" value="Whatever Text I Want" />
			<evar number="5" value="{video.customFields['episode-name']}" />
			<eventNumbers value="4" />
		</event>
		
		<event name="mediaPause">
			<prop number="1" value="{video.customFields['my-custom-field']}" />
			<prop number="2" value="{video.customFields['showname']}" />
			<prop number="3" value="{video.displayName}" />
